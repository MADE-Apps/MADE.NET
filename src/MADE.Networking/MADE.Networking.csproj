<Project Sdk="MSBuild.Sdk.Extras">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Product>MADE.NET Networking</Product>
    <Description>
      MADE.NET Networking comes fully loaded with wrappers for easily executing network requests from applications, handling the responses, as well as providing extensions for common URI scenarios.
      A perfect companion to any application handling networking.
    </Description>
    <PackageTags>MADE Networking Extensions Json Stream HttpClient</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\MADE.Runtime\MADE.Runtime.csproj" />
    <ProjectReference Include="..\MADE.Threading\MADE.Threading.csproj" />
  </ItemGroup>

</Project>
